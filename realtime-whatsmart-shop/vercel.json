{
  "version": 2,
  "routes": [
    {
      "src": "/webhook",
      "dest": "api/index.py"
    },
    {
      "src": "/success",
      "dest": "/public/success.html"
    },
    {
      "src": "/cancel",
      "dest": "/public/cancel.html"
    },
    {
      "src": "/(.*)",
      "dest": "/public/$1"
    }
  ]
}
